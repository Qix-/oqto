/**
 * Token lists for the Oqto CCS grammar
 */

/**
 * Environment selectors
 */
environment_token "environment selector token"
	= 'arch'		/* arch */
	/ 'os'			/* platform */
	/ 'system'		/* type */
	/ 'release'		/* release */
	;

/**
 * Directive tokens
 */
directive_token "directive token"
	= 'import'		/* include a file */
	/ 'use'			/* use a module */
	;